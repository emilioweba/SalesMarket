<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.SalesMarket" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <preference name="loglevel" value="DEBUG" />
    <feature name="SocialMessage">
        <param name="android-package" value="uk.co.ilee.socialmessage.SocialMessage" />
    </feature>
    <feature name="SocialSharing">
        <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
    </feature>
    <name>SalesMarket</name>
    <description>A blank project that uses Apache Cordova to help you build an app that targets multiple mobile platforms: Android, iOS, Windows, and Windows Phone.</description>
    <author email="emilioweba@gmail.com" href="http://www.emilioweba.com">Emilio Weba</author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="SplashScreen" value="screen" />
    <preference name="windows-target-version" value="8.0" />
    <preference name="windows-phone-target-version" value="8.1" />
    <vs:plugin name="org.apache.cordova.geolocation" version="0.3.10" />
    <vs:plugin name="cordova-plugin-x-socialsharing" />
    <vs:plugin name="cordova-plugin-social-message" version="0.3.1" />
    <vs:platformSpecificValues />
</widget>
